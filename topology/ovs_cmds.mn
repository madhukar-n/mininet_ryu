sh ovs-vsctl add-br s4
sh ip link add s4-eth1 type veth peer name s3-eth4
sh ip link add s4-eth2 type veth peer name s2-eth4
sh ovs-vsctl add-port s4 s4-eth1
sh ovs-vsctl add-port s2 s2-eth4
sh ovs-vsctl add-port s3 s3-eth4
sh ip link set s3-eth4 up
sh ip link set s2-eth4 up
sh ip link set s4-eth1 up
sh ovs-vsctl set-controller s4c tcp:127.0.0.1:6653

